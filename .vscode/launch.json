    "version": "0.2.0",
    "configurations": [          
        {
            "type": "java",
            "name": "AccessApplication",
            "request": "launch",
            "mainClass": "com.teracontrol.access.AccessApplication",
            "projectName": "access"
        },
        {
            "type": "java",
            "name": "DnUSimulatorApplication",
            "request": "launch",
            "mainClass": "com.dnusimulator.dnusimulator.DnUSimulatorApplication",
            "projectName": "dnusimulator"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**",
                "${workspaceFolder}/node_modules/**/*.js"
            ],
            "program": "${workspaceFolder}/app.js"
        }
    ]
}